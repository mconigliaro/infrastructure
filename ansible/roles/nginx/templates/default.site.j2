server {
  listen 80 default_server;
  listen [::]:80 default_server;

  return 301 https://$host$request_uri;
}

server {
  listen 443 ssl http2;
  listen [::]:443 ssl http2;

  include /etc/nginx/ssl_params;

  {% for path, url in nginx_proxy_paths.items() %}
  location /{{ path }} {
    include /etc/nginx/proxy_params;
    proxy_pass {{ url }};
  }

  {% endfor %}
}
