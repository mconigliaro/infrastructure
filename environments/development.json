{
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "name": "development",
  "description": "Development",
  "override_attributes": {
    "apache": {
      "default_redirect_path": "http://example.org"
    },
    "apcupsd": {
      "device": "",
      "isconfigured": "no",
      "upscable": "usb",
      "upstype": "usb"
    },
    "backuppc": {
      "hosts": {
        "localhost": {
          "XferMethod": "\"rsync\"",
          "RsyncShareName": "[\"/home\"]"
        },
        "mail.example.org": {
          "PingPath": "\"/bin/echo\"",
          "XferMethod": "\"rsync\"",
          "RsyncShareName": "[\"/home\"]"
        }
      },
      "users": [
        "example"
      ]
    },
    "chef": {
      "client": {
        "chef_server_url": "https://api.opscode.com/organizations/example",
        "daemon_enabled": true,
        "interval": 3600,
        "splay": 60,
        "validation_client_name": "example-validator"
      }
    },
    "ddclient": {
      "custom": "yes",
      "hosts": [
        "home.example.com",
        "mail.example.com",
        "home.example.org",
        "mail.example.org"
      ],
      "login": "example",
      "password": "example",
      "run_daemon": "true"
    },
    "mail": {
      "mailname": "home.example.org",
      "postfix": {
        "aliases": [
          "root: example"
        ],
        "inet_interfaces": ["all"],
        "maps": {
          "access_sender": [
            "user@example.com REJECT Comment"
          ],
          "sasl_password": [
            "smtp.example.net user:password"
          ],
          "virtual": [
            "example@example.com  example",
            "example@example.org  example",
            "@example.org         example"
          ]
        },
        "relayhost": "[smtp.example.net]:587",
        "virtual_alias_domains": ["example.com", "example.org"]
      }
    },
    "mdadm": {
      "arrays": {
        "/dev/md0": {
          "devices": ["/dev/sdb1", "/dev/sdc1"],
          "level": 1,
          "action": ["create", "assemble"]
        }
      },
      "mounts": {
        "/mnt/backups": {
          "device": "/dev/md0",
          "action": ["enable", "mount"]
        }
      }
    },
    "minidlna": {
      "media_dirs": [
        "/home/example"
      ]
    },
    "nfs": {
      "exports": {
        "/srv/nfs/home": "*(rw,sync,no_subtree_check,no_root_squash)",
        "/srv/nfs": "*(rw,sync,fsid=0,crossmnt,no_subtree_check,no_root_squash)"
      },
      "mounts": {
        "/home": "/srv/nfs/home"
      }
    },
    "samba": {
      "workgroup": "EXAMPLE"
    },
    "smartmontools": {
      "start_smartd": "yes"
    },
    "ubuntu": {
      "timezone": "America/Denver"
    }
  }
}
